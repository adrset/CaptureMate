<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import gui.AnchorPaneExtended?>
<?import javafx.scene.control.Button?>
<BorderPane fx:id="anchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="gui.Controller">
    <children>
        <ImageView fx:id="imageView"/>
        <HBox fx:id="buttonContainer">
            <Button alignment="BOTTOM_RIGHT" minWidth="50" minHeight="25" fx:id="closeButton" text="close" onAction="#onCloseButton"/>
            <Button alignment="BOTTOM_RIGHT" minWidth="50" minHeight="25" fx:id="resizeButton" text="+/-" onAction="#onResizeButton"/>
        </HBox>
    </children>
</BorderPane>